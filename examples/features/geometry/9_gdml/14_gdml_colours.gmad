! this model is from pyg4ometry
! pyg4ometry/pyg4ometry/test/pythonCompoundExamples/lhc_blm_model.py
p1: placement, geometryFile="gdml:blm_model_colours.gdml", x=0.1*m, z=0.5*m,
    	       axisAngle=1, axisX=1, angle=-pi/12;

d1: drift, l=1*m;
l1: line=(d1);
use, l1;

beam, particle="e-",
      energy=1.3*GeV,
      distrType="sphere",
      S0=0.5*m;

option, preprocessGDML=0;