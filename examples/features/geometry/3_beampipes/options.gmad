option,	physicsList="em",
	defaultRangeCut=1*cm,	
	prodCutPositrons=1*cm,
	prodCutElectrons=1*cm,
	prodCutPhotons=1*cm,
	checkOverlaps=1;

option, ngenerate=1;

! we allow the normal magnet geometry as it affects the construction of the drift

! the default integrator set bdsimmatrix removes physical construction of angled faces
! turn this off by selecting another integrator for better testing here
option, integratorSet="bdsimtwo";

beam, particle="proton",
      energy=10.0*GeV;