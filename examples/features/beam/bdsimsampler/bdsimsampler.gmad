beam,  particle = "e-",
       energy = 1.0*GeV,
       distrType = "bdsimsampler:d1_1",
       distrFile = "../../data/sample1.root";

d1: drift, l=0.25;
q1: quadrupole, l=0.5, k1=2.0;
q2: quadrupole, l=0.5, k1=-2.0;
lat: line = (d1,q1,d1,d1,q2,d1);
use, lat;

sample, all;

! 10 events in file - 4 have 1 particle in sampler so need more than that
option, ngenerate=10;

option, physicsList="all_particles";

option, seed=123;