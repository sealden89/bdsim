blueish: newcolour, red=0, green=123, blue=255, alpha=0.05;
box: rcol, fieldAll="f1", l=1*m, material="air", horizontalWidth=10*cm, colour="blueish";
l1: line=(box);
use, l1;

beam, particle="e-",
      energy=3*GeV,
      distrType="gauss",
      sigmaX=30*cm,
      sigmaY=30*cm,
      sigmaXp=1e-6,
      sigmaYp=1e-6;

option, ngenerate=1;

option, outputFormat="none";